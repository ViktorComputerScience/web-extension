{
    "manifest_version": 3,
    "name": "web-extension",
    "version": "0.0.1",
    "permissions": [
        "tabs"
    ],
    "action": {
        "default_popup": "popup.html",
        "default_icon": "green-icon-128.png"
    },
    "background": {
        "service_worker": "background.js"
    }, 
    "host_permissions": [
        "http://*/*",
        "https://*/*"
    ]
}