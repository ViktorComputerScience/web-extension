{
    "manifest_version": 3,
    "name": "web-extension",
    "version": "1.0.0",
    "permissions": [
        "tabs",
        "storage"
    ],
    "action": {
        "default_popup": "popup.html",
        "default_icon": "green-icon-128.png"
    },
    "background": {
        "service_worker": "background.js"
    }, 
    "host_permissions": [
        "http://*/*",
        "https://*/*"
    ],
    "icons": {
        "48": "extension-logo-48.png",
        "128": "extension-logo-128.png"
    }
}